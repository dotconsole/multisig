    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@openzeppelin/contracts": "^4.3.2",
    "@typechain/ethers-v5": "^7.1.2",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai": "^4.2.22",
    "@types/jest": "^27.0.2",
    "@types/node": "16.11.11",
    "@types/react": "17.0.37",
    "@types/react-dom": "^17.0.9",
    "@types/react-icons": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
